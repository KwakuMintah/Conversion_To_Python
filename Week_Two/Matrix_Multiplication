import numpy as np

A = [[1,2],[3,4]]
B = [[5,6],[7,8]]
C = [[0,0],[0,0]]
D = [[0,0],[0,0]]
aRows = len(A[0])

for i in range(aRows):
    for j in range(aRows):
        C[i][j] = A[i][j] + B[i][j]

for i in range(aRows):
    for j in range(aRows):
        for k in range(aRows):
            D[i][j] = A[i][k] + B[k][j]


print(C)
print(D)

